{
  "name": "eco-gesto",
  "version": "2.0.0",
  "description": "Sistema interativo de geração sonora via gestos corporais com evolução genética e integração Music.AI",
  "type": "module",
  "main": "js/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest tests/unit --coverage",
    "test:integration": "jest tests/integration --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:all": "npm run test && npm run test:e2e",
    "lint": "eslint js/ tests/",
    "lint:fix": "eslint js/ tests/ --fix",
    "format": "prettier --write \"**/*.{js,json,md,html,css}\"",
    "format:check": "prettier --check \"**/*.{js,json,md,html,css}\"",
    "prepare": "husky install",
    "validate": "npm run lint && npm run format:check && npm run test"
  },
  "keywords": [
    "gesture-recognition",
    "audio-synthesis",
    "genetic-algorithm",
    "web-audio-api",
    "mediapipe",
    "music-ai",
    "interactive-music",
    "generative-art"
  ],
  "author": "Equipe Eco-Gesto",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/seu-usuario/Eco-Gesto.git"
  },
  "bugs": {
    "url": "https://github.com/seu-usuario/Eco-Gesto/issues"
  },
  "homepage": "https://github.com/seu-usuario/Eco-Gesto#readme",
  "dependencies": {
    "axios": "^1.6.2",
    "idb": "^8.0.0",
    "lamejs": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.5",
    "cypress": "^13.6.1",
    "cypress-axe": "^1.5.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "vite": "^5.0.7"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "js/**/*.js",
      "!js/config/**",
      "!js/**/*.config.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,html,css}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
